{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style" : 0,
        "import/extensions" : 0,
        "import/prefer-default-export" : 0,
        "no-alert" : 0,
        "no-console" : 0,
        "no-plusplus" : 0,
        "no-use-before-define" : 0,
        "import/no-extraneous-dependencies" : 0,
        "no-underscore-dangle" : 0,
        "no-undef" : 0,
        "import/no-unresolved" : 0,
        "no-unused-vars" : 0,
        "import/no-cycle": 0,
        "class-methods-use-this": 0,
        "import/order": 0,
        "eol-last": 0,
        "import/no-useless-path-segments": 0,
        "no-shadow": 0,
        "prefer-const": 0,
        "comma-dangle": 0,
        "import/no-named-as-default-member": 0,
        "eqeqeq": 0,
        "no-inner-declarations": 0,
        "max-len": 0,
        "no-param-reassign": 0,
        "no-unreachable ": 0,
        "no-empty": 0
    }
}
